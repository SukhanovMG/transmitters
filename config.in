# Размер передаваемых блоков (байты).
BlockSize = 1024

# Битрейт (кбит/с).
Bitrate = 1024

# Время, за которое считается средний битрейт (секунды).
AverageBitrateCalculationTime = 1

# Процент, на который максимально может отличаться битрейт у клиента (%).
BitrateDiff = 10

# Продолжительность теста (секунды).
# Для успешного завершения теста, битрейт должен оставаться в допустимых пределах втечение этого времени.
TestTime = 10

# Флаг использования пула предвыделенной памяти для блоков.
UseMemPool = @FOR_CONFIG_USE_MEMPOOL@

# Флаг копирования блока при передаче от главного потока к рабочему
CopyBlockOnTransfer = @FOR_CONFIG_COPY_BLOCK_ON_TRANSFER@

# Флаг использования jemalloc
UseJeMalloc = @FOR_CONFIG_USE_JEMALLOC@

# Флаг использования libev
UseLibev = @FOR_CONFIG_USE_LIBEV@
ReturnPointersThroughPipes = @FOR_CONFIG_RETURN_POINTERS_THROUGH_PIPES@

UseSpinlock = false
