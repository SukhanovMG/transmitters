# Размер передаваемых блоков (байты).
BlockSize = 1024

# Битрейт (кбит/с).
Bitrate = 1024

# Время, за которое считается средний битрейт (секунды).
AverageBitrateCalculationTime = 1

# Процент, на который максимально может отличаться битрейт у клиента (%).
BitrateDiff = 10

# Продолжительность теста (секунды).
# Для успешного завершения теста, битрейт должен оставаться в допустимых пределах втечение этого времени.
TestTime = 10

# Флаг копирования блока при передаче от главного потока к рабочему
CopyBlockOnTransfer = false

# Флаг использования пула предвыделенной памяти для блоков.
UseMemPool = @CFG_MEMPOOL@

# Тип очереди
# kTmQueueSimple = 0 - обычная очередь на основе списка
# kTmQueueSimpleMempool = 1 - та же очередь, но элементы очереди берутся из пула
# kTmQueueRbuf = 2 - очередь, основанная на циклическом буфере
QueueType = @CFG_QUEUE_TYPE@

# Снижение конкуренции за счётчик ссылок копированием
OptimizeRefcountUseByCopy = true
# Использование мьютекса в счётчике ссылок
RefCountWithMutex = false

# Тип модуля потоков
# TMThreadType_Simple = 0 - императивная парадигма
# TMThreadType_Libev_queue = 1 - событийно-ориентированная парадигма, очередь
# TMThreadType_Libev_pipe = 2 - событийно-ориентированная парадигма, канал
ThreadModuleType = @CFG_THREAD_MODULE_TYPE@

# Только для libev с каналами, возврат блоков по каналам
ReturnPointersThroughPipes = true
